local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/OptusN1/VENYX-UI-2/main/VENYX%20UI%20%3D-%3D"))()
local uii = library.new("LAST PIRATES | A I M E R I Z", 5013109572)

 
local mainpage = uii:addPage("Main Page", 7040391851)
local farmsection = mainpage:addSection("Auto Farm")
farmsection:addToggle("Auto Farm Level [ Under ]", false, function(a)
	_G.autofarmUnder = a
	
	task.spawn(function()
		while task.wait() do
			if not _G.autofarmUnder then break end
			pcall(function()
				local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
				AutoQuest()
                AutoHaki()
				CheckLevel()
				for i, v in pairs(game.Workspace.Lives:GetChildren()) do
					if v.Name == MONSTER and v:FindFirstChild("Humanoid") then
                        plr.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,-8,0) * CFrame.Angles(math.rad(90),0,0)
						click()
                        AutoEquipWeapon()
					if not v:FindFirstChild("Humanoid") then
						plr.Anchored = true
					else 
						plr.Anchored = false
					end
					end
				end
			end)
		end
	end)
end)
farmsection:addToggle("Auto Farm Level [ Top ]", false, function(a)
	_G.autofarmTop = a
	
	task.spawn(function()
		while task.wait() do
